<script>
// CDN平台配置 - 在这里自定义链接
const cdnPlatforms = {
    'edgeone-pages': {
        svgPath: '/assets/svg/edgeone-pages.svg',
        link: 'https://edgeone.qq.com/',
        name: 'EdgeOne Pages'
    },
    'cloudflare': {
        svgPath: '/assets/svg/Cloudflare.svg',
        link: 'https://www.cloudflare.com/',
        name: 'Cloudflare'
    },
    'vercel': {
        svgPath: '/assets/svg/Vercel.svg',
        link: 'https://vercel.com/',
        name: 'Vercel'
    }
};

// CDN平台检测函数
async function detectCdnPlatform() {
    try {
        // 获取当前页面的响应标头
        const response = await fetch(window.location.href, {
            method: 'GET',
            credentials: 'same-origin',
            cache: 'no-cache',
            headers: {
                'Accept': 'text/html'
            }
        });
        
        // 获取server标头
        const serverHeader = response.headers.get('server');
        
        // 检测平台
        let platform = null;
        if (serverHeader) {
            if (serverHeader.includes('edgeone-pages')) {
                platform = 'edgeone-pages';
            } else if (serverHeader.toLowerCase().includes('cloudflare')) {
                platform = 'cloudflare';
            } else if (serverHeader.includes('Vercel')) {
                platform = 'vercel';
            }
        }
        
        // 显示对应图标
        const container = document.getElementById('cdn-platform-container');
        if (container) {
            // 清除现有内容
            container.innerHTML = '';
            
            // 如果检测到平台，显示对应图标
            if (platform && cdnPlatforms[platform]) {
                const config = cdnPlatforms[platform];
                
                // 创建链接
                const link = document.createElement('a');
                link.href = config.link;
                link.target = '_blank';
                link.rel = 'noopener noreferrer';
                
                // 创建SVG图标
                const svg = document.createElement('img');
                svg.style.height = '1em';
                svg.style.width = 'auto';
                svg.style.verticalAlign = 'middle';
                svg.style.display = 'inline-block';
                svg.style.maxWidth = '100%';
                
                // 设置SVG属性
                svg.src = config.svgPath;
                svg.alt = config.name + ' CDN';
                svg.loading = 'eager';
                
                // 处理加载错误
                svg.onerror = function() {
                    this.style.display = 'none';
                };
                
                // 添加到容器
                link.appendChild(svg);
                container.appendChild(link);
            }
        }
    } catch (error) {
        console.error('CDN platform detection failed:', error);
        // 出错时不显示任何图标
    }
}

// 页面加载后检测
if (typeof window !== 'undefined') {
    // 使用setTimeout确保DOM已完全加载
    setTimeout(detectCdnPlatform, 500);
    
    // 窗口加载完成后再次检测
    window.addEventListener('load', detectCdnPlatform);
    
    // 页面可见性变化时重新检测
    if (typeof document !== 'undefined' && document.addEventListener) {
        document.addEventListener('visibilitychange', function() {
            if (!document.hidden) {
                detectCdnPlatform();
            }
        });
    }
}
</script>
